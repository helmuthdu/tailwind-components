{
  "name": "@windtail/components",
  "private": true,
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/tailwind-components.umd.js",
  "module": "./dist/tailwind-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/tailwind-components.es.js",
      "require": "./dist/tailwind-components.umd.js"
    }
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "tsc && vite build -c=vite.lib.config.js",
    "preview": "vite preview",
    "storybook": "npm run tailwind:storybook && start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "tailwind:storybook": "tailwindcss --postcss -i tailwind.css -o .storybook/public/tailwind.css -m"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-essentials": "^6.4.12",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/html": "^6.4.12",
    "@tailwindcss/typography": "^0.5.0",
    "@uppercod/vite-meta-url": "^1.5.1",
    "@uppercod/vite-meta-url-load-css": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.1",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  }
}
