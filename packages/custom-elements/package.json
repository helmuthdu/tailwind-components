{
  "name": "@windtail/components",
  "private": true,
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/tailwind-components.umd.js",
  "module": "./dist/tailwind-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/tailwind-components.es.js",
      "require": "./dist/tailwind-components.umd.js"
    }
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "tsc && vite build -c=vite.lib.config.js",
    "preview": "vite preview",
    "storybook": "npm run tailwind:storybook && start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "tailwind:storybook": "tailwindcss --postcss -i tailwind.css -o .storybook/public/tailwind.css -m"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/theming": "^6.4.19",
    "@storybook/html": "^6.4.19",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.40",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "cssnano": "^5.1.4",
    "postcss": "^8.4.8",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.3",
    "react": "^17.0.2",
    "tailwindcss": "^3.0.23",
    "ts-dedent": "^2.2.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  }
}
